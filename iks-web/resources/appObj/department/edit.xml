<?xml version="1.0"?>
<editView appObj="department" xmlns="http://com.iks.org/schema/edit/1.0">
  <fieldbox>
    <textbox fieldName="name"/>
  </fieldbox>
  <tabbox>
    <tab label="Employees">
      <referenceTableField fieldName="employees" referenceGrid="employee_grid">
        <columns>
          <column fieldName="employees.id" label="Employee Id"/>
          <column fieldName="employees.fio" label="Employee Name"/>
        </columns>
      </referenceTableField>
    </tab>
  </tabbox>
</editView>