    <button id="button2" label="Button 2"/>
    <spacer flex="2"/>
    <button id="button3" label="Button 3"/>
    <vbox pack="end" align="stretch">
      <button id="button4" label="Button 4" style="width: 150px"/>
      <button id="button5" label="Button 5"/>
      <button id="button6" label="Button 6"/>
    </vbox>
  </hbox>
</editView>