<data appObj="employee" tableName="employee">
  <field tableField="id" name="id" label="Id" primary="true"/>
  <field tableField="fio" name="fio" label="First Name/Last Name" required="true"/>
  <field tableField="gender" name="gender" label="Sex">
    <options>
      <option value="male">Male</option>
      <option value="female">Female</option>
    </options>
  </field>
  <manyToOne name="user" appObj="user" tableField="user_id" referenceField="id"/>
</data>