<?xml version="1.0"?>
<data appObj="employee" tableName="employee" xmlns="http://com.iks.org/schema/model/1.0">
  <field tableField="id" fieldName="id" label="Id" primary="true"/>
  <field tableField="fio" fieldName="fio" label="First Name/Last Name" required="true"/>
  <selectField tableField="gender" fieldName="gender" label="Sex">
    <options>
      <option value="male">Male</option>
      <option value="female">Female</option>
    </options>
  </selectField>
  <field tableField="hired_date" fieldName="hiredDate" label="Hired" type="date"/>
  <field tableField="hired_time" fieldName="hiredTime" label="Hired Time" type="time"/>
  <oneToOne fieldName="user" appObj="user" tableField="user_id"/>
  <manyToOne fieldName="department" appObj="department" tableField="department_id"/>
</data>