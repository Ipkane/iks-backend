<?xml version="1.0"?>
<listView appObj="user" xmlns="http://com.iks.org/schema/list/1.0">
  <appTable id="user_grid" appObj="user">
    <filterPanel>
      <hbox>
        <checkbox name="active"/>
      </hbox>
    </filterPanel>
    <columns>
      <column name="id" label="User Id"/>
      <column name="username" label="Username"/>
      <column name="active"/>
    </columns>
  </appTable>
  <tabbox>
    <tab label="Roles">
      <appTable appObj="role" id="user_role_grid">
        <columns>
          <column name="id" label="Role Id"/>
          <column name="name" label="Name"/>
        </columns>
      </appTable>
    </tab>
  </tabbox>
</listView>