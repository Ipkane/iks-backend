<?xml version="1.0"?>
<editView appObj="user" xmlns="http://com.iks.org/schema/edit/1.0">
  <fieldbox>
    <textbox name="id" readonly="true"/>
    <textbox name="username"/>
    <textbox name="password"/>
    <checkbox name="active"/>
  </fieldbox>
  <tabbox>
    <tab label="Roles">
      <referenceTable name="roles" appObj="role">
        <columns>
          <column name="id" label="Role Id"/>
          <column name="name" label="Name"/>
        </columns>
      </referenceTable>
    </tab>
  </tabbox>
</editView>